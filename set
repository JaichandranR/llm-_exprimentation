    format: |
      [
        {% set today = now() | date('yyyy-MM-dd') %}
        {% set m1 = now() | date('yyyy-MM-01') %}
        {"index": 0, "date": "{{ today }}"},
        {"index": 1, "date": "{{ m1 }}"},
        {% for i in range(1, 8) %}
          {% set d = now() | dateAdd(-i, 'MONTHS') | date('yyyy-MM-01') %}
          {"index": {{ i + 1 }}, "date": "{{ d }}"}{% if not loop.last %},{% endif %}
        {% endfor %}
      ]
