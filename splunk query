index=* "app-id-90177-dep-id-114232-uu-id-gtcatyp2zje" "explicit deny" "s3:GetObject"
| rex "User:\s(?<arn>arn:aws:sts::\d+:assumed-role/[^\s]+)"
| rex "Request ID:\s(?<request_id>[A-Z0-9]+)"
| rex "Status Code:\s(?<status_code>\d+)"
| rex "ErrorMessage:\s(?<error_message>[^\n]+)"
| stats count by arn request_id status_code error_message
| sort -count
