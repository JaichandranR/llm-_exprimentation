WITH random_rows AS (
    SELECT sealcomponentid
    FROM cosmos_nonhcd_iceberg_prototype.common_data_prototype."32010_seal_component"
    ORDER BY rand()
    LIMIT 100
)
UPDATE cosmos_nonhcd_iceberg_prototype.common_data_prototype."32010_seal_component" AS t
SET cosmosingestedat = t.cosmosingestedat - INTERVAL '1' YEAR
WHERE t.sealcomponentid IN (SELECT sealcomponentid FROM random_rows);
